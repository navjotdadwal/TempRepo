<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.dadwalsocialmedia">

    <application
        tools:replace="android:label"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="DadwalSocialMedia"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DadwalSocialMedia">
        <activity
            android:name=".CreatePostActivity"
            android:exported="false" />
        <activity android:name=".auth.AuthenticationActivity"></activity>
        <activity android:name=".CommentsActivity"></activity>
        <activity android:name=".notes.AddEditNoteActivity"></activity>
        <activity android:name=".notes.MainnotesActivity"></activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>